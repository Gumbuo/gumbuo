# WalletConnect Project ID (get from https://cloud.walletconnect.com/)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here

# RPC URL for Ethereum Mainnet (for server-side contract reads)
RPC_URL=https://mainnet.infura.io/v3/YOUR_INFURA_KEY
# Or use a public RPC:
# RPC_URL=https://rpc.ankr.com/eth

# Gumbuo Token Contract Address (Base Network)
NEXT_PUBLIC_GMB_CONTRACT=0xeA80bCC8DcbD395EAf783DE20fb38903E4B26dc0

# Optional: Infura API Key (if using Infura for RPC)
NEXT_PUBLIC_INFURA_API_KEY=your_infura_api_key_here

# Optional: For Vercel deployment
# VERCEL_TOKEN=your_vercel_token_here

# Optional: Allowed Origins for production
# NEXT_PUBLIC_ALLOWED_ORIGINS=https://gumbuo.io
