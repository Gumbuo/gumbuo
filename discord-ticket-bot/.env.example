# Discord Bot Token (from Discord Developer Portal)
BOT_TOKEN=your_bot_token_here

# Guild (Server) ID
GUILD_ID=your_server_id_here

# Staff Role ID (users with this role can manage tickets)
STAFF_ROLE_ID=your_staff_role_id_here

# Ticket Category ID (where ticket channels will be created)
TICKET_CATEGORY_ID=your_category_id_here

# Transcript Channel ID (where closed ticket logs will be sent)
TRANSCRIPT_CHANNEL_ID=your_transcript_channel_id_here
